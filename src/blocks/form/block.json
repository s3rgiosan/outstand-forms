{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "osf/form",
  "version": "0.1.0",
  "title": "Outstand Forms",
  "category": "common",
  "icon": "",
  "description": "",
  "keywords": [
    "outstand",
    "form",
    "feedback",
    "contact"
  ],
  "textdomain": "outstand-forms",
  "attributes": {
    "formId": {
      "type": "string"
    },
    "method": {
      "type": "string",
      "default": "post"
    },
    "action": {
      "type": "string"
    },
    "requiredIndicator": {
      "type": "string",
      "default": "*"
    },
    "labelPosition": {
      "type": "string",
      "default": "top"
    },
    "helpTextPosition": {
      "type": "string",
      "default": "bottom"
    }
  },
  "providesContext": {
    "osf/formId": "formId",
    "osf/labelPosition": "labelPosition",
    "osf/helpTextPosition": "helpTextPosition",
    "osf/requiredIndicator": "requiredIndicator"
  },
  "allowedBlocks": [
    "osf/form-fields",
    "osf/form-submit-button"
  ],
  "supports": {
    "html": false,
    "anchor": true,
    "className": false,
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "lineHeight": true
    },
    "interactivity": true
  },
  "editorStyle": "file:./index.css",
  "editorScript": "file:./index.js",
  "viewScriptModule": "file:./view.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
